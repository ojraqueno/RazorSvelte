{
  "name": "svelterazoraspnet",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dotnet-watch": "dotnet watch",
    "dotnet-run": "dotnet run",
    "dotnet-clean": "dotnet clean",
    "dotnet-build": "dotnet build",
    "frontend-scss-build": "sass ./Styles/style.scss ./wwwroot/style.css --no-source-map --style compressed",
    "frontend-scss-watch": "sass ./Styles/style.scss ./wwwroot/style.css -style compressed --watch",
    "frontend-index-build": "rollup -c ./Pages/Index.rollup.config.js",
    "frontend-index-watch": "rollup -c ./Pages/Index.rollup.config.js -w",
    "frontend-build-all": "node ./Scripts/build-all.js",
    "npm-upgrade": "ncu --upgrade && npm install && npm rebuild node-sass && npm audit fix",
    "npm-audit-fix": "npm audit fix"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.2",
    "@rollup/plugin-replace": "^3.0.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@tsconfig/svelte": "^3.0.0",
    "monaco-editor": "^0.31.1",
    "rollup": "^2.62.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-monaco-editor": "^0.1.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.45.2",
    "svelte": "^3.44.3",
    "svelte-check": "^2.2.11",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  }
}
